<script>
  $('#iagree').on('click', function () {
    console.log('ffefefe')
    $('.chat-head').css('display', 'none');
    $('.support-ticket-info').css('display', 'block');
    $('.support-ticket-content').css('display', 'block');
    $('.support-ticket-content-w').css('display', 'block');
    $.ajax({
      type: 'POST',
      url: '/test',
      data: {},
      dataType: "json",
      success: function (response) {
       console.log(response.name.length)
        var len = response.name.length; 
       for (let index = 0; index < response.name.length; index++) {
         const element = response.name[index];
         
       
          if (element == 'X Signature') {
            
            var item = $('<div style="position:absolute; top:'+response.top[index] +'px;left:'+response.left[index]+'px;">Signature<select><option value =1>ijende</option></select></div>');
            $('#droppable').append(item);

          }
          if (element == 'X Initials') {
            var item = $('<div style="position:absolute; top:'+response.top[index] +'px;left:'+response.left[index]+'px;">Signature<select><option value =1>ijende</option></select></div>');
            $('#droppable').append(item);
          }
          if (element == 'X Text') {
            var textbox = $('<textarea style="width:150px; position:absolute; top:'+response.top[index] +'px;left:'+response.left[index]+'px;" rows="2" cols="50">');
              $('#droppable').append(textbox);
            }
            if (element == 'X Date') {
              var date = $('<input style="position:absolute; top: 100px;left: 330px;" type="date"id="date" >');
              $('#droppable').append(date);
            }
        }
        $('#trstsid').text(response.trstsid);
        $('#docdes').text(response.description);
        $('#doctitle').text(response.title);
        $('#image').attr('src', response.location);

      }

    });
  });

